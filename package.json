{
  "name": "free-demo",
  "private": true,
  "version": "0.0.0",
  "main": "electron/main.js",
  "description": "紫小艾应用",
  "author": "Your Name",
  "productName": "紫小艾",
  "scripts": {
    "dev": "vite",
    "build": "vite build --emptyOutDir",
    "preview": "vite preview",
    "electron:serve": "concurrently \"npm run dev\" \"sleep 5 && cross-env NODE_ENV=development electron .\"",
    "electron:build": "cross-env NODE_ENV=production npm run build && electron-builder",
    "electron:build:win": "cross-env NODE_ENV=production npm run build && electron-builder --win --x64",
    "electron:build:mac": "cross-env NODE_ENV=production npm run build && electron-builder --mac",
    "electron:build:linux": "cross-env NODE_ENV=production npm run build && electron-builder --linux",
    "electron:start": "electron ."
  },
  "build": {
    "appId": "com.yourcompany.zixiaoai",
    "productName": "紫小艾",
    "copyright": "Copyright © 2025 ${author}",
    "electronVersion": "39.2.4",
    "directories": {
      "output": "electron_dist",
      "buildResources": "public"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json",
      "node_modules/**/*"
    ],
    "extraResources": [
      {
        "from": "public/music",
        "to": "public/music",
        "filter": [
          "**/*.mp3"
        ]
      }
    ],
    "asar": true,
    "asarUnpack": [
      "**/*.node",
      "**/*.dll",
      "dist/**/*",
      "node_modules/element-plus/**/*",
      "public/music/**"
    ],
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "public/home.png",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "public/home.ico",
      "requestedExecutionLevel": "highestAvailable",
      "artifactName": "${productName}-${version}-setup.${ext}",
      "executableName": "紫小艾"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm",
        "zip"
      ],
      "icon": "public/home.png",
      "category": "Utility",
      "artifactName": "${productName}-${version}-linux-x64.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "紫小艾",
      "installerIcon": "public/home.ico",
      "uninstallerIcon": "public/home.ico",
      "installerHeaderIcon": "public/home.ico",
      "license": "LICENSE.txt",
      "artifactName": "${productName}-${version}-setup.${ext}"
    }
  },
  "dependencies": {
    "@chenfengyuan/vue-countdown": "^2.1.3",
    "@vue-flow/background": "^1.3.2",
    "@vue-flow/controls": "^1.1.2",
    "@vue-flow/core": "^1.45.0",
    "@vue-flow/minimap": "^1.5.3",
    "axios": "^1.7.9",
    "crypto-js": "^4.2.0",
    "decimal.js": "^10.4.3",
    "element-plus": "^2.9.0",
    "fabric": "^4.6.0",
    "html2canvas": "^1.4.1",
    "loudness": "^0.4.2",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rete": "^2.0.6",
    "rete-area-plugin": "^2.1.5",
    "rete-auto-arrange-plugin": "^2.0.2",
    "rete-connection-path-plugin": "^2.0.4",
    "rete-connection-plugin": "^2.0.5",
    "rete-context-menu-plugin": "^2.0.6",
    "rete-engine": "^2.1.1",
    "rete-history-plugin": "^2.1.1",
    "rete-react-plugin": "^2.1.0",
    "rete-render-utils": "^2.0.3",
    "rete-vue-plugin": "^2.1.2",
    "simple-keyboard": "^3.8.26",
    "simple-keyboard-layouts": "^3.4.51",
    "swiper": "^11.2.5",
    "vue": "^3.5.25",
    "vue-js-box": "^1.3.5",
    "vue-router": "^4.5.0",
    "vue-video-player": "^5.0.1",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12",
    "http-proxy-middleware": "^3.0.5",
    "postcss-px-to-viewport-8-plugin": "^1.2.5",
    "postcss-pxtorem": "^6.1.0",
    "postcss-scss": "^4.0.9",
    "sass": "^1.85.1",
    "sass-embedded": "^1.86.0",
    "unplugin-auto-import": "^19.1.1",
    "unplugin-element-plus": "^0.9.1",
    "unplugin-vue-components": "^28.4.1",
    "vite": "^6.0.1",
    "vite-plugin-imagemin": "^0.6.1",
    "wait-on": "^9.0.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}